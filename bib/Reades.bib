	doi = {10.1111/joes.12267},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/joes.12267},
	journal = {Journal of Economic Surveys},
	keywords = {Bibliometrics, Input--output methods, Spatial statistics, Theory of agglomerations},
	number = {1},
	pages = {60-84},
	title = {Bibliometric Analysis of the Quantitative Methods Applied to the Measurement of Industrial Clusters},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/joes.12267},
	volume = {33},
	year = {2019},
	bdsk-url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/joes.12267},
