@article{Wrigley:1986,
	abstract = {In this paper the definition and significance of 'citation classics' in the geographical literature is discussed. Drawing upon a study of the citation records of all articles published in four key geography journals during the ten-year period after publication, a 'within-discipline' definition of the level of citation at which a geography article achieves 'classic' status is proposed, and an equivalent definition for books is also proposed. Initial lists of twenty of the most cited articles and books published by geographers are set out, and methods of identifying 'potential citation classics' amongst recent publications are considered.},
	author = {Neil Wrigley and Stephen Matthews},
	date-added = {2020-03-30 20:33:01 +0100},
