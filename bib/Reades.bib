@article{Scheider:2020,
	abstract = {Abstract ``Data Science'' has taken many disciplines by storm. And for a good reason: New forms and unseen quantities of data enter nearly every scientific field, substantially changing the ways how scientists do science, and potentially allowing them to answer old questions or to pose them in novel ways. The recent success of Data Science is also reflected in corresponding study programs and curricula and the emergence of specialized branches, such as Geographic Data Science (GDS). Some researchers, therefore, claim that Data Science and GDS should be treated as autonomous scientific disciplines, while others fear that it sells nothing but old wine in new bottles. In an attempt to sober the discussion, we investigate GDS and Data Science from the perspective of meta-science. We provide arguments why today's GDS and Data Science should be seen as an interdisciplinary community of practice of data-driven scientists, rather than a scientific discipline. We also discuss what is missing for GDS and Data Science to become genuine scientific disciplines.},
	author = {Scheider, Simon and Nyamsuren, Enkhbold and Kruiger, Han and Xu, Haiqi},
	date-added = {2020-09-20 08:50:35 +0100},
	date-modified = {2020-09-20 08:50:44 +0100},
	doi = {10.1111/gec3.12537},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/gec3.12537},
	journal = {Geography Compass},
	keywords = {community of practice, geographic data science, Geography, GIScience, meta-science, scientific concepts, scientific questions},
	number = {n/a},
	pages = {e12537},
	title = {Why geographic data science is not a science},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/gec3.12537},
	volume = {n/a},
	bdsk-url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/gec3.12537},
